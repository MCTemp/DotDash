//line 76
function code2letter (code) {
  for (x=0;x<32;x++) {
    if (code == mc[x]) {
      break
    }
  }
  return lt[x]
}
 



























//line 113
function dotdash(){
  if (elapsed<6) {
    CodeLtr = CodeLtr + "•"
  } else {
    CodeLtr = CodeLtr + "-"
  }
  Cod.change(CodeLtr)
}













//line 135
function word() {
  val = "□"
  send(val)
  MyMsg = MyMsg + val
  Msg.change(MyMsg)
}
